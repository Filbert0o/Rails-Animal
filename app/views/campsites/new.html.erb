<h1>Campsite form for new one</h1>

<%= form_for @campsite do |f| %>

<fieldset>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :owner %>
  <%= f.text_field :owner %>

  <%= f.label :description %>
  <%= f.text_field :description %>
</fieldset>

<%= f.submit "Add Campsite" %>
<% end %>
